{
  "name": "son_q",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "next:dev": "next dev",
    "build": "pnpm --filter \"@son-q/*\" build && next build",
    "next:start": "next start",
    "packages:build": "pnpm --filter \"@son-q/*\" build",
    "packages:typecheck": "pnpm --filter \"@son-q/*\" typecheck",
    "packages:clean": "pnpm --filter \"@son-q/*\" exec rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "biome:check": "biome check .",
    "biome:ci": "biome ci .",
    "biome:lint": "biome lint .",
    "biome:lint:fix": "biome lint --write .",
    "biome:format": "biome format --write .",
    "biome:format:check": "biome format .",
    "tsc": "tsc --noEmit",
    "clean:next": "rm -rf .next",
    "clean:cache": "rm -rf node_modules/.cache"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@son-q/api": "workspace:*",
    "@son-q/queries": "workspace:*",
    "@son-q/types": "workspace:*",
    "@son-q/ui-tailwind": "workspace:*",
    "@son-q/utils": "workspace:*",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "es-toolkit": "^1.30.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "nookies": "^2.5.2",
    "notistack": "^3.0.2",
    "prop-types": "latest",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-youtube": "^10.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@son-q/config": "workspace:*",
    "@vitest/coverage-v8": "^3.2.4",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "sass": "^1.89.2",
    "tw-animate-css": "^1.3.5",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "24.3.0",
    "npm": "11.4.2"
  }
}
