{
  "name": "son_q",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm run build:packages && next dev",
    "build": "pnpm --filter \"@son-q/*\" build && next build",
    "build:packages": "pnpm --filter \"@son-q/*\" build",
    "start": "next start",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "check": "biome check .",
    "check:ci": "biome ci .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "pnpm --filter \"@son-q/*\" typecheck && tsc --noEmit",
    "clean": "pnpm --filter \"@son-q/*\" exec rm -rf dist",
    "clean:all": "pnpm run clean && rm -rf .next && rm -rf node_modules/.cache",
    "dev:clean": "pnpm run clean:all && pnpm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@son-q/api": "workspace:*",
    "@son-q/queries": "workspace:*",
    "@son-q/types": "workspace:*",
    "@son-q/ui": "workspace:*",
    "@son-q/utils": "workspace:*",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "es-toolkit": "^1.30.0",
    "next": "15.3.4",
    "nookies": "^2.5.2",
    "notistack": "^3.0.2",
    "prop-types": "latest",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-youtube": "^10.1.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@son-q/config": "workspace:*",
    "@vitest/coverage-v8": "^3.2.4",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "sass": "^1.89.2",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "24.3.0",
    "npm": "11.4.2"
  }
}
